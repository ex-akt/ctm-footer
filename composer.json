{
  "name": "ex-akt/ctm-footer",
  "type": "symfony-bundle",
  "description": "Footer bundle for Contao Theme-Manager",
  "keywords": ["contao", "theme"],
  "homepage": "https://www.ex-akt.de/",
  "authors": [
    {
      "name": "ex-akt",
      "homepage": "https://ex-akt.de/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "contao-thememanager/core": "^2.2",
    "contao/core-bundle": "^4.13 || ^5.3"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.3.1"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "ContaoThemeManager\\Footer\\": "src/"
    },
    "classmap": [
      "contao/"
    ],
    "exclude-from-classmap": [
      "contao/config/",
      "contao/dca/",
      "contao/languages/",
      "contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "ContaoThemeManager\\Footer\\ContaoManager\\Plugin"
  }
}
